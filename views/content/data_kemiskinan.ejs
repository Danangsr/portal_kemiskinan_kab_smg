  let dataTable1 = [{label:'BLT DD',y:0},{label:'RTLH desa',y:0}
]
try {
  

  let dana_desa_BLT_DD = await $.post(`/kemiskinan/cari`,{tahun_data:$('#tahun2').val(),
   padan_dukcapil:'Padan',
              VERVAL_memenuhi_kriteria_P3KE:'Ya',  Id_kec:$('#Id_kec').val()
  ,Id_des_kel:$('#Id_des_kel').val(),
  desil_kesejahteraan:$('#desil_kesejahteraan').val(),dana_desa_BLT_DD:'Ya',count:1})
  let dana_desa_RTLH_desa = await $.post(`/kemiskinan/cari`,{tahun_data:$('#tahun2').val(),
   padan_dukcapil:'Padan',
              VERVAL_memenuhi_kriteria_P3KE:'Ya',  Id_kec:$('#Id_kec').val()
  ,Id_des_kel:$('#Id_des_kel').val(),
  desil_kesejahteraan:$('#desil_kesejahteraan').val(),dana_desa_RTLH_desa:'Ya',count:1})

  dataTable1[0].y = dana_desa_BLT_DD.data[0].jml
  dataTable1[1].y = dana_desa_RTLH_desa.data[0].jml

  var chart9 = new CanvasJS.Chart("chartContainer9", {
    animationEnabled: true,
    axisX:{
        labelFontSize: 14,
        interval: 1,
        // labelMaxWidth: 100,
    // labelWrap: true,
        // labelAngle: 30,
    },
    axisY:{
        labelFontSize: 14,
    },                
      data: [{
        type: "column",  

          dataPoints: dataTable1
      }]
  });
  chart9.render();
  $('#loading10').hide()

} catch (error) {
  console.log(error);
}
}
