                Id_des_kel:$('#Id_des_kel').val(),
                padan_dukcapil:'Padan',
  VERVAL_memenuhi_kriteria_P3KE:'Ya',

              }
              let hsl = await $.post('/kemiskinan/cari',data)
              let hsl2 = await $.post('/kemiskinan/jml_kk',data)

              $('#desil'+i).html(`${hsl.data[0].jml} Jiwa` )
              $('#desil'+i+'2').html(`${hsl2.data[0].total_kk} KK` )

            }
            let data1 = {
            group:'kecamatan',
              tahun_data:$('#tahun').val(),
                     Id_kec:$('#Id_kec').val(),
                Id_des_kel:$('#Id_des_kel').val(),
                padan_dukcapil:'Padan',
  VERVAL_memenuhi_kriteria_P3KE:'Ya',
            }
            let kecamatan = []
           await $.post('/kemiskinan/cari',data1).then(function (hsl) {
              kecamatan = hsl.data              
           })

