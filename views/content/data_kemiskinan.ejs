                       Id_kec:$('#Id_kec').val(),
                Id_des_kel:$('#Id_des_kel').val(),
                padan_dukcapil:'Padan',
  VERVAL_memenuhi_kriteria_P3KE:'Ya',
                desil_kesejahteraan:k,
                wadmkc:kecamatan[i].label
              }
              await $.post('/kemiskinan/cari',data).then(function (hsl) {
                kecamatan[i]['desil'+k] = hsl.data[0].jml
              })
      
            }
